{
  "name": "eclipsefdn-iot-eclipse-org",
  "description": "Source code for iot.eclipse.org",
  "main": "packages.js",
  "author": "Christopher Guindon",
  "license": "EPL-2.0",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/EclipseFdn/iot.eclipse.org/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/EclipseFdn/iot.eclipse.org.git"
  },
  "scripts": {
    "dev": "NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "install": "NODE_ENV=production npm run yaml2json_adopters && npm run production",
    "optimize-iot-json": "NODE_ENV=production node bin/scraper/src/index.js",
    "postinstall": "NODE_ENV=production npm run fetch_project",
    "build_netlify": "NODE_ENV=production npm run production && npm run optimize-iot-json && npm run fetch_project",
    "yaml2json_adopters": "./bin/adopters.sh",
    "fetch_project": "NODE_ENV=production node node_modules/eclipsefdn-hugo-solstice-theme/bin/projects/index.js -l data"
  },
  "dependencies": {
    "eclipsefdn-hugo-solstice-theme": "0.0.57",
    "js-yaml": "^3.13.1",
    "json-minify": "^1.0.0"
  }
}
